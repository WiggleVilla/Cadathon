each $cat in edr, iq, pro {\n        &.vc-#{$cat} {\n          background-image: url(get-button($cat, $type, false));\n          background-position: center;\n          background-repeat: no-repeat;\n          background-size: contain;\n\n          &:hover {\n            background-image: url(get-button($cat, $type, true));\n          }\n        }\n      }\n    }\n  }\n\n  .vc-link {\n    height: $link-height;\n    box-sizing: border-box;\n    display: table;\n    width: 100%;\n\n    a {\n      display: table-cell;\n      vertical-align: middle;\n    }\n\n    .vc-actions {\n      display: table-cell;\n      width: 200px;\n      text-align: right;\n      padding-right: 16px;\n      //height: $link-height;\n      //float: right;\n\n      .vc-preview, .vc-download {\n        display: inline-block;\n        width: $button-width;\n        height: $button-height;\n        margin-top: ($link-height - $button-height) / 2;\n        margin-left: 10px;\n      }\n    }\n  }\n\n  #vc-left {\n    max-height: 500px;\n    overflow-y: scroll;\n  }\n\n  #vc-right {\n    height: 500px;\n    flex-grow: 1;\n    border: 1px solid #efefef;\n\n    .vc-preview-help-text {\n      align-items: center;\n      display: flex;\n      height: 100%;\n      justify-content: center;\n      width: 100%;\n\n      img {\n        margin-left: 5px;\n        margin-right: 5px;\n      }\n    }\n\n    .vc-preview {\n      width: 100px;\n      height: (80 / 150) * 100px;\n      margin: 0 10px 0 10px;\n\n      &.vc-edr {\n        height: (80 / 136) * 100px;\n      }\n    }\n\n    iframe {\n      border: 0;\n      max-height: 500px !important;\n    }\n  }\n}\n\n@media screen and (min-width: $two-column-screen-min) {\n  .vc-tab {\n    #vc-left {\n      width: 50%;\n    }\n  }\n}\n\n@media screen and (max-width: $two-column-screen-min) {\n  .vc-tab {\n    flex-direction: column-reverse;\n\n    .vc-link {\n      display: table;\n\n      padding-bottom: 10px;\n      border-bottom: 1px solid #eaeaea;\n\n      & > a {\n        display: table-cell;\n        vertical-align: center;\n      }\n\n      .vc-actions {\n        width: 75px;\n\n        a {\n          display: block;\n        }\n      }\n    }\n  }\n}\n\n@media screen and (max-width: 880px) and (min-width: $two-column-screen-min) {\n  .vc-tab.tab-content {\n    .vc-link {\n      border-bottom: 1px solid #eaeaea;\n      padding: 16px 0 16px 0;\n    }\n  }\n}\n'],sourceRoot:""}])}]);

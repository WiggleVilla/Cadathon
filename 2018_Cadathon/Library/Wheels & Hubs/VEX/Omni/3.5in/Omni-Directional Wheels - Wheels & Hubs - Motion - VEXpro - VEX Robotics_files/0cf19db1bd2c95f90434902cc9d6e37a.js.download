		window.handleInputCrossInstant = function (input) {
			if (input.val().length > 0) {
				input.closest('#instant-search-box').find('.clear-query-instant').show();
			}
			else {
				input.closest('#instant-search-box').find('.clear-query-instant').hide();
			}
		};


		});

		$(document).on('click', '.clear-query-autocomplete', function () {
			var input = $(this).closest('#algolia-searchbox').find('input');
			input.val('');

			if (algoliaConfig.autocomplete.enabled && !algoliaConfig.instant.enabled) {
